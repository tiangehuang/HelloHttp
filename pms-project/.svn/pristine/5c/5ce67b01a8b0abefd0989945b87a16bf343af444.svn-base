package com.epro.pms.project_basic_info.po;
/**
 * 骨干员工信息bean
 * @author qinshiguang
 *
 */
public class ProjCorePersonInfo
{
	private Integer id;					//id
	private Integer projectId;			//项目ID
	private String eproNo;				//用户ID
	private String name;				//名字
	private String planRole;			//规划角色
	private Integer seperationLevel;	//离职风险
	private Integer isSeperation;		//是否离职
	private String desc;				//具体情况
	private String stabilityMeasure;	//骨干稳定措施
	public Integer getId()
	{
		return id;
	}
	public void setId(Integer id)
	{
		this.id = id;
	}
	public Integer getProjectId()
	{
		return projectId;
	}
	public void setProjectId(Integer projectId)
	{
		this.projectId = projectId;
	}
	public String getEproNo()
	{
		return eproNo;
	}
	public void setEproNo(String eproNo)
	{
		this.eproNo = eproNo;
	}
	public String getName()
	{
		return name;
	}
	public void setName(String name)
	{
		this.name = name;
	}
	public String getPlanRole()
	{
		return planRole;
	}
	public void setPlanRole(String planRole)
	{
		this.planRole = planRole;
	}
	public Integer getSeperationLevel()
	{
		return seperationLevel;
	}
	public void setSeperationLevel(Integer seperationLevel)
	{
		this.seperationLevel = seperationLevel;
	}
	public Integer getIsSeperation()
	{
		return isSeperation;
	}
	public void setIsSeperation(Integer isSeperation)
	{
		this.isSeperation = isSeperation;
	}
	public String getDesc()
	{
		return desc;
	}
	public void setDesc(String desc)
	{
		this.desc = desc;
	}
	public String getStabilityMeasure()
	{
		return stabilityMeasure;
	}
	public void setStabilityMeasure(String stabilityMeasure)
	{
		this.stabilityMeasure = stabilityMeasure;
	}
	@Override
	public String toString()
	{
		return "ProjCorePersonInfo [id=" + id + ", projectId=" + projectId + ", eproNo=" + eproNo + ", name=" + name
				+ ", planRole=" + planRole + ", seperationLevel=" + seperationLevel + ", isSeperation=" + isSeperation
				+ ", desc=" + desc + ", stabilityMeasure=" + stabilityMeasure + "]";
	}
}
